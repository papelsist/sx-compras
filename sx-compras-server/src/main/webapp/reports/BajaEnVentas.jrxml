	<parameter name="FECHA_FIN" isForPrompting="true" class="java.util.Date"/>
	<parameter name="DIAS" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(30)]]></defaultValueExpression>
	</parameter>
	<parameter name="VALOR_VENTA" isForPrompting="true" class="java.math.BigDecimal"/>
	<parameter name="ORIGEN" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA["%"]]></defaultValueExpression>
	</parameter>
	<parameter name="PORCENTAJE" isForPrompting="true" class="java.lang.Double"/>
	<parameter name="SUCURSAL" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT A.NOMBRE,A.MESES,A.TIPO
,ROUND(SUM(A.VENTA_NETA),2) AS VENTA_NETA
,ROUND(SUM(A.PROM_VTA),2) AS PROM_VTA
,ROUND(SUM(A.PERIODO),2) AS PERIODO 
,ROUND(SUM(A.PERIODO)*100/SUM(A.PROM_VTA),1) AS PORC_VTA
,MAX(A.ULT_VTA) AS ULT_VTA
FROM (
SELECT 'HST' AS PRDO,(CASE WHEN V.CLIENTE_ID=8 THEN 'MOSTRADOR' ELSE v.cliente END) nombre
,(CASE 	WHEN 	MAX((CASE WHEN v.origen = 'CRE' THEN 'CRE' ELSE 'CON' END)) =  'CON' THEN 'CON' ELSE 'CRE' END) AS TIPO
,sum(v.IMP_NETO) as venta_neta 
,round((to_days(DATE_ADD($P{FECHA_FIN}, INTERVAL -(30+1) DAY))-(to_days($P{FECHA_INI})))/30.4167,2) as meses
,sum(v.IMP_NETO)/round((to_days(DATE_ADD($P{FECHA_FIN}, INTERVAL -(30+1) DAY))-(to_days($P{FECHA_INI})))/30.4167,2) as prom_vta
,0 as periodo
,max(v.fecha) as ULT_VTA
FROM fact_ventasdet V  
WHERE V.FECHA BETWEEN $P{FECHA_INI} AND DATE_ADD($P{FECHA_FIN}, INTERVAL -(30+1) DAY)
AND V.SUCURSAL_ID LIKE $P{SUCURSAL} AND (CASE WHEN V.ORIGEN='CRE' THEN 'CREDITO' ELSE 'CONTADO' END) LIKE $P{ORIGEN}
group by V.CLIENTE,V.FECHA 
union
SELECT 'ACT' as PRDO,(CASE WHEN V.CLIENTE_ID=8 THEN 'MOSTRADOR' ELSE v.cliente END) nombre
,(CASE 	WHEN 	MAX((CASE WHEN v.origen = 'CRE' THEN 'CRE' ELSE 'CON' END)) =  'CON' THEN 'CON' ELSE 'CRE' END) AS TIPO
,0 as venta_neta
,0 as meses
,0 as prom_vta
,sum(v.IMP_NETO) as periodo
,max(v.fecha) as ULT_VTA
FROM bi.fact_ventasdet V  
WHERE V.FECHA BETWEEN DATE_ADD($P{FECHA_FIN}, INTERVAL -30 DAY) AND $P{FECHA_FIN}
AND V.SUCURSAL_ID LIKE $P{SUCURSAL} AND (CASE WHEN V.ORIGEN='CRE' THEN 'CREDITO' ELSE 'CONTADO' END) LIKE $P{ORIGEN}
group by V.CLIENTE 
) AS A 
GROUP BY A.NOMBRE,A.TIPO 
HAVING SUM(A.PROM_VTA)>=$P{VALOR_VENTA} AND ROUND(SUM(A.PERIODO),2)>0
AND (ROUND(SUM(A.PERIODO)*100/SUM(A.PROM_VTA),1))<=$P{PORCENTAJE}
ORDER BY  $P!{ORDER} $P!{FORMA}]]></queryString>

