FROM cancelacion_de_cfdi V JOIN CFDI F ON(V.CFDI_ID=F.ID) JOIN cuenta_por_cobrar C ON(C.CFDI_ID=F.ID)
JOIN SUCURSAL S ON (S.ID=C.SUCURSAL_ID) join cliente l on  (C.cliente_id=l.id)
WHERE C.FECHA=$P{FECHA} AND C.cancelada IS NOT NULL
AND C.tipo_documento='VENTA' 
order by C.TIPO,C.DOCUMENTO]]></queryString>

