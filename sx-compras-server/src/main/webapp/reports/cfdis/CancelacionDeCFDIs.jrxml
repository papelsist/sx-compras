	<queryString><![CDATA[SELECT C.TIPO,F.FOLIO,C.FECHA AS FECHA_FAC,C.CANCELADA,l.CLAVE,l.NOMBRE,S.CLAVE AS SUC,S.NOMBRE AS NOM_SUC
,CASE WHEN C.TIPO IN('COD','CON') THEN 'CONTADO' WHEN C.TIPO='CRE' THEN 'CREDITO' ELSE 'SIN CLASIF' END AS ORIGEN
,C.forma_de_pago AS FPAGO,F.TOTAL,C.TOTAL TOT_CXC,C.TIPO_DOCUMENTO,V.date_created CREADO,V.last_updated MODIFICADO
