	<queryString>
		<![CDATA[SELECT A.DESCRIPCION_ER DESCRIPCION,A.MES_NOMBRE AS MES
,   CASE WHEN SUBSTR(A.DESCRIPCION_ER,1,2) IN('01','02','03','04','05','06') THEN 'A'
         WHEN SUBSTR(A.DESCRIPCION_ER,1,2) IN('07') THEN 'B'
         WHEN SUBSTR(A.DESCRIPCION_ER,1,2) IN('08') THEN 'C'
         WHEN SUBSTR(A.DESCRIPCION_ER,1,2) IN('09','10') THEN 'D'
         WHEN SUBSTR(A.DESCRIPCION_ER,1,2) IN('11','12','13') THEN 'E'
         WHEN SUBSTR(A.DESCRIPCION_ER,1,2) IN('14','15') THEN 'F'
    ELSE 'SG' END AS GRUPO
,SUM(CASE WHEN A.YEAR=$P{YEAR}-1 AND A.MES=$P{MES} THEN (CASE WHEN SUBSTR(A.DESCRIPCION_ER,1,2)='10' THEN A.SALDO ELSE (A.SALDO) END) ELSE 0 END) AS MES_ANT
,SUM(CASE WHEN A.YEAR=$P{YEAR} AND A.MES=$P{MES} THEN (CASE WHEN SUBSTR(A.DESCRIPCION_ER,1,2)='10' THEN A.SALDO ELSE (A.SALDO) END) ELSE 0 END) AS MES_ACT
,SUM(CASE WHEN A.YEAR=$P{YEAR}-1 AND A.MES<=$P{MES} THEN (CASE WHEN SUBSTR(A.DESCRIPCION_ER,1,2)='10' THEN A.SALDO ELSE (A.SALDO) END) ELSE 0 END) AS YEAR_ANT
,SUM(CASE WHEN A.YEAR=$P{YEAR} AND A.MES<=$P{MES} THEN (CASE WHEN SUBSTR(A.DESCRIPCION_ER,1,2)='10' THEN A.SALDO ELSE (A.SALDO) END) ELSE 0 END) AS YEAR_ACT
,A.VTA_CRE_MES_ANT,A.VTA_NETA_MES_ANT,A.VTA_CRE_YEAR_ANT,A.VTA_NETA_YEAR_ANT
,A.VTA_CRE_MES_ACT,A.VTA_NETA_MES_ACT,A.VTA_CRE_YEAR_ACT,A.VTA_NETA_YEAR_ACT
FROM  (
SELECT S.EJERCICIO YEAR,S.MES,(case when s.mes=1 then 'ENERO' when s.mes=2 then 'FEBRERO'	when s.mes=3 then 'MARZO' when s.mes=4 then 'ABRIL' when s.mes=5 then 'MAYO' when s.mes=6 then 'JUNIO'
	when s.mes=7 then 'JULIO' when s.mes=8 then 'AGOSTO' when s.mes=9 then 'SEPTIEMBRE' when s.mes=10 then 'OCTBRE' when s.mes=11 then 'NOVIEMBRE' when s.mes=12 then 'DICIEMBRE' else 'ND' end) AS MES_NOMBRE
,C.ID,C.PADRE_ID,C.CLAVE,( CASE WHEN C.ID=344 THEN '01 VENTAS BRUTAS CREDITO'
        WHEN C.ID=348 THEN '02 DEVOLUCIONES CREDITO'
        WHEN C.ID=351 THEN '03 DESCUENTOS CREDITO'
        WHEN C.ID=345 THEN '04 VTAS RELACIONADAS'
        WHEN C.ID=2573 THEN '05 DEVS RELACIONADAS'
        WHEN C.ID=2575 THEN '06 DESCTS REALACIONADAS'
        WHEN C.ID IN(342,343,346,347,349,350) THEN '07 VENTAS NETAS CONTADO'
        WHEN C.ID=1697 THEN '08 COSTO DE VENTA'
        WHEN C.PADRE_ID in(51,52,53) AND C.ID<>420 THEN '09 GASTOS DE OPERACION'
        WHEN  C.ID=420 THEN '10 GASTOS DE OPERACION2'
        WHEN C.PADRE_ID=55 THEN '11 PRODUCTOS FINANCIEROS'
        WHEN C.PADRE_ID=54 THEN '12 GASTOS FINANCIEROS'
        WHEN C.PADRE_ID IN(57,48,50,56) THEN '13 OTRO INGRESOS/GASTOS'
        WHEN C.PADRE_ID=58 THEN '14 I.S.R.'
	WHEN C.PADRE_ID=59 THEN '15 P.T.U.'
    ELSE "SD"   END ) AS DESCRIPCION_ER
,ROUND(SUM(-(S.DEBE-S.HABER))/1000,0) AS SALDO
,IFNULL((SELECT ROUND(SUM(-(S.DEBE-S.HABER))/1000,0) AS SALDO FROM  cuenta_contable C JOIN saldo_por_cuenta_contable S ON(S.CUENTA_ID=C.ID) WHERE C.ID=344 AND S.MES= $P{MES} AND S.EJERCICIO= $P{YEAR}-1 ),0) AS VTA_CRE_MES_ANT
,IFNULL((SELECT ROUND(SUM(-(S.DEBE-S.HABER))/1000,0) AS SALDO FROM  cuenta_contable C JOIN saldo_por_cuenta_contable S ON(S.CUENTA_ID=C.ID) WHERE C.PADRE_ID IN(45,46,47) AND S.MES= $P{MES} AND S.EJERCICIO= $P{YEAR}-1 ),0) AS VTA_NETA_MES_ANT
,IFNULL((SELECT ROUND(SUM(-(S.SALDO_FINAL))/1000,0) AS SALDO FROM  cuenta_contable C JOIN saldo_por_cuenta_contable S ON(S.CUENTA_ID=C.ID) WHERE C.ID=344 AND S.MES= $P{MES} AND S.EJERCICIO= $P{YEAR}-1 ),0) AS VTA_CRE_YEAR_ANT
,IFNULL((SELECT ROUND(SUM(-(S.SALDO_FINAL))/1000,0) AS SALDO FROM  cuenta_contable C JOIN saldo_por_cuenta_contable S ON(S.CUENTA_ID=C.ID) WHERE C.PADRE_ID IN(45,46,47) AND S.MES= $P{MES} AND S.EJERCICIO= $P{YEAR}-1 ),0) AS VTA_NETA_YEAR_ANT
,IFNULL((SELECT ROUND(SUM(-(S.DEBE-S.HABER))/1000,0) AS SALDO FROM  cuenta_contable C JOIN saldo_por_cuenta_contable S ON(S.CUENTA_ID=C.ID) WHERE C.ID=344 AND S.MES= $P{MES} AND S.EJERCICIO= $P{YEAR} ),0) AS VTA_CRE_MES_ACT
,IFNULL((SELECT ROUND(SUM(-(S.DEBE-S.HABER))/1000,0) AS SALDO FROM  cuenta_contable C JOIN saldo_por_cuenta_contable S ON(S.CUENTA_ID=C.ID) WHERE C.PADRE_ID IN(45,46,47) AND S.MES= $P{MES} AND S.EJERCICIO= $P{YEAR} ),0) AS VTA_NETA_MES_ACT
,IFNULL((SELECT ROUND(SUM(-(S.SALDO_FINAL))/1000,0) AS SALDO FROM  cuenta_contable C JOIN saldo_por_cuenta_contable S ON(S.CUENTA_ID=C.ID) WHERE C.ID=344 AND S.MES= $P{MES} AND S.EJERCICIO= $P{YEAR} ),0) AS VTA_CRE_YEAR_ACT
,IFNULL((SELECT ROUND(SUM(-(S.SALDO_FINAL))/1000,0) AS SALDO FROM  cuenta_contable C JOIN saldo_por_cuenta_contable S ON(S.CUENTA_ID=C.ID) WHERE C.PADRE_ID IN(45,46,47) AND S.MES= $P{MES} AND S.EJERCICIO= $P{YEAR} ),0) AS VTA_NETA_YEAR_ACT
FROM  cuenta_contable C
JOIN saldo_por_cuenta_contable S ON(S.CUENTA_ID=C.ID)
WHERE C.NIVEL=2  AND C.CLAVE IN (401,402,403,404,501,505,600,613,614,701,702,703,704,750,755) AND S.MES<= $P{MES} AND ( S.EJERCICIO= $P{YEAR}-1 OR S.EJERCICIO= $P{YEAR} )
GROUP BY S.ejercicio,S.MES,DESCRIPCION_ER
ORDER BY MES DESC
) AS A GROUP BY A.DESCRIPCION_ER]]>
	</queryString>
