	<queryString><![CDATA[SELECT tipo,id,ejercicio,mes,cta_clave,cta_descripcion,saldo_inicial,debe,haber,saldo_final
,folio,clase,subtipo,fecha,descripcion,MIN(a.claveMin) minClave,MAX(a.claveMax) maxClave
FROM (
SELECT 'A' tipo,s.id,s.ejercicio,s.mes,c.clave cta_clave,c.descripcion cta_descripcion,s.saldo_inicial
,(SELECT ifnull(SUM(x.debe),0)  FROM poliza_det x join cuenta_contable y on(x.cuenta_id=y.id) join poliza p on(x.poliza_id=p.id)
where y.detalle is true and y.id=c.id and p.fecha BETWEEN CONCAT(YEAR($P{FECHA_INI}),'/',MONTH($P{FECHA_INI}),'/',01) and (DATE(DATE_ADD($P{FECHA_INI}, INTERVAL -1 DAY)))  ) debe
,(SELECT ifnull(SUM(x.haber),0)  FROM poliza_det x join cuenta_contable y on(x.cuenta_id=y.id) join poliza p on(x.poliza_id=p.id)
where y.detalle is true and y.id=c.id and p.fecha BETWEEN CONCAT(YEAR($P{FECHA_INI}),'/',MONTH($P{FECHA_INI}),'/',01) and (DATE(DATE_ADD($P{FECHA_INI}, INTERVAL -1 DAY)))  ) haber
,0 saldo_final,0 folio,'SALDO_INICIAL: ' clase
,'SALDO_INICIAL: ' subtipo,date(CONCAT(cast(s.ejercicio as char(4)),'/',cast(s.mes as char(4)),'/01')) fecha,'SALDO INICIAL: ' AS descripcion
,(SELECT min(c.clave) FROM saldo_por_cuenta_contable s join cuenta_contable c on(s.cuenta_id=c.id) where c.detalle is true and s.ejercicio=year('2019-06-01') and mes = month('2019-06-01') AND C.CLAVE BETWEEN $P{CUENTA_INI} AND $P{CUENTA_FIN} ) claveMin
,(SELECT max(c.clave) FROM saldo_por_cuenta_contable s join cuenta_contable c on(s.cuenta_id=c.id) where c.detalle is true and s.ejercicio=year('2019-06-01') and mes = month('2019-06-01') AND C.CLAVE BETWEEN $P{CUENTA_INI} AND $P{CUENTA_FIN} ) claveMax
FROM saldo_por_cuenta_contable s join cuenta_contable c on(s.cuenta_id=c.id)
where c.detalle is true and s.ejercicio=year($P{FECHA_INI}) and mes = month($P{FECHA_INI}) AND C.CLAVE BETWEEN $P{CUENTA_INI} AND $P{CUENTA_FIN} 
group by s.id
union
SELECT 'B' tipo,s.id,p.ejercicio,p.mes,c.clave,c.descripcion,0 saldo_inicial,s.debe,s.haber,0 saldo_final,
p.folio,(case when p.tipo='DIARIO' then 'Dr-' when p.tipo='INGRESO' then 'Ig-' else 'Eg-' end) clase,p.subtipo,p.fecha,s.descripcion
,(SELECT min(c.clave) FROM saldo_por_cuenta_contable s join cuenta_contable c on(s.cuenta_id=c.id) where c.detalle is true and s.ejercicio=year('2019-06-01') and mes = month('2019-06-01') AND C.CLAVE BETWEEN $P{CUENTA_INI} AND $P{CUENTA_FIN} ) claveMin
,(SELECT max(c.clave) FROM saldo_por_cuenta_contable s join cuenta_contable c on(s.cuenta_id=c.id) where c.detalle is true and s.ejercicio=year('2019-06-01') and mes = month('2019-06-01') AND C.CLAVE BETWEEN $P{CUENTA_INI} AND $P{CUENTA_FIN} ) claveMax
FROM poliza_det s join cuenta_contable c on(s.cuenta_id=c.id) join poliza p on(s.poliza_id=p.id)
where c.detalle is true and p.fecha BETWEEN $P{FECHA_INI} and  $P{FECHA_FIN} AND C.CLAVE BETWEEN $P{CUENTA_INI} AND $P{CUENTA_FIN} 
group by s.id
) as a
group by a.tipo,a.id]]></queryString>

