	<parameter name="POLIZA_ID" isForPrompting="true" class="java.lang.Long"/>
	<queryString><![CDATA[SELECT (case when d.metodo_de_pago='02' then 'CHEQUE' when d.metodo_de_pago='03' then 'TRANSFERENCIA' else 'OTRO_PAGO' end) fpago,d.id,p.id as poliza_id,p.tipo,p.subtipo,p.folio,p.fecha
,d.cta_origen,d.banco_origen,d.beneficiario,d.rfc
,d.moneda,d.tip_camb, d.monto_total_pago*d.tip_camb as monto_mxn, d.monto_total_pago as monto_total
,d.cta_destino,d.banco_destino,d.referencia,d.metodo_de_pago,d.asiento descrip_pago,p.concepto,p.sucursal
FROM  poliza_det d 
join poliza p on(p.id=d.poliza_id)
where d.rfc is not null and p.id=$P{POLIZA_ID}
group by fpago,d.origen,d.banco_origen,p.fecha,d.beneficiario,d.rfc,d.monto_total_pago,d.moneda,d.tip_camb]]></queryString>

