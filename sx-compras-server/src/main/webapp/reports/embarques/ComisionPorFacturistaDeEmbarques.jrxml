,X.COMISION/100 COMISION,X.PRECIO_TONELADA,X.IMPORTE_COMISION,X.VALOR_CAJAS
FROM traslado E 
JOIN envio_comision X ON(X.traslado_id=E.ID)
JOIN CHOFER C ON(C.ID=E.chofer_ID)
join facturista_de_embarque P ON(C.facturista_id=P.id)
JOIN sucursal S ON(S.ID=E.sucursal_id)
WHERE  P.ID=$P{FACTURISTA} AND DATE(X.REGRESO) BETWEEN $P{FECHA_INI} AND $P{FECHA_FIN}
) AS A 
GROUP BY A.CHOFER
UNION
SELECT 'POR SUCURSAL' TIPO,FACTURISTA,'SUCURSAL' VTA,CHOFER,SUCURSAL,SUM(VALOR) VALOR,SUM(VALOR_CAJAS) CAJAS,SUM(VALOR+(VALOR_CAJAS*2)) IMPORTE,SUM(KILOS) KILOS,SUM(MANIOBRA) FLETE
,(CASE WHEN COMISION>0 THEN COMISION ELSE PRECIO_TONELADA END) FACTOR,SUM(IMPORTE_COMISION) IMP_COMISION,0.00 SALDO,0.00 INT_E_IVA,0 MATERIAL,0 CELULAR,0 PATIN,0 MANIOBRA_LOCAL,0 MANIOBRA_FORANEA,0 VALES,0 OTROS,0.00 DESCTO
FROM (
SELECT P.NOMBRE FACTURISTA,S.NOMBRE AS SUCURSAL,C.NOMBRE AS CHOFER ,F.VALOR ,0 AS CANTIDAD,F.KILOS 
,CASE WHEN X.PRECIO_TONELADA>0 THEN 'POR_TONELADA' WHEN F.tipo_documento='CRE' THEN 'CREDITO' ELSE 'CONTADO' END AS VTA
,IFNULL((SELECT V.cargos_por_maniobra FROM VENTA V WHERE V.ID=F.ORIGEN),0) AS FLETE
,X.ID,X.MANIOBRA,X.COMISION/100 COMISION,X.PRECIO_TONELADA,X.IMPORTE_COMISION,X.VALOR_CAJAS
from embarque E 
JOIN envio F ON(E.ID=F.EMBARQUE_ID)
JOIN envio_comision X ON(X.ENVIO_ID=F.ID)
JOIN CHOFER C ON(C.ID=E.chofer_ID)
join facturista_de_embarque P ON(C.facturista_id=P.id)
