UNION

SELECT 'XDESGLOSE' TIPO,FACTURISTA,'FACTURISTA' VTA,CHOFER,SUCURSAL,SUM(VALOR) VALOR,SUM(VALOR_CAJAS) CAJAS,SUM(VALOR+(VALOR_CAJAS*2)) IMPORTE,SUM(KILOS) KILOS,SUM(MANIOBRA) FLETE
,(CASE WHEN COMISION>0 THEN COMISION ELSE PRECIO_TONELADA END) FACTOR,SUM(IMPORTE_COMISION) IMP_COMISION,SALDO
,(SELECT SUM(C.TOTAL)-SUM(IFNULL((SELECT SUM(A.IMPORTE) FROM aplicacion_de_cobro A WHERE A.cuenta_por_cobrar_id=C.ID),0)) 
	FROM cuenta_por_cobrar C JOIN cliente T ON(C.cliente_id=T.ID) JOIN facturista_de_embarque F ON(F.RFC=T.RFC) 
	WHERE F.ID=A.FACT_ID AND C.TIPO='CHO' AND C.tipo_documento='NOTA_DE_CARGO' AND C.FECHA>'2019-04-26') INT_E_IVA
,MATERIAL,CELULAR,PATIN,MANIOBRA_LOCAL,MANIOBRA_FORANEA,VALES,OTROS
,CASE WHEN SALDO=0.0 THEN 0.0 WHEN SALDO<=round((ROUND((SUM(IMPORTE_COMISION)+SUM(MANIOBRA))*1.12,2)*0.30),0) THEN SALDO ELSE round((ROUND((SUM(IMPORTE_COMISION)+SUM(MANIOBRA))*1.12,2)*0.30),0) END DESCTO
FROM (
SELECT P.ID FACT_ID,P.NOMBRE FACTURISTA,S.NOMBRE AS SUCURSAL,C.NOMBRE AS CHOFER ,F.VALOR ,0 AS CANTIDAD,F.KILOS 
,CASE WHEN X.PRECIO_TONELADA>0 THEN 'POR_TONELADA' WHEN F.tipo_documento='CRE' THEN 'CREDITO' ELSE 'CONTADO' END AS VTA
,IFNULL((SELECT V.cargos_por_maniobra FROM VENTA V WHERE V.ID=F.ORIGEN),0) AS FLETE
,X.ID,X.MANIOBRA,X.COMISION/100 COMISION,X.PRECIO_TONELADA,X.IMPORTE_COMISION,X.VALOR_CAJAS
,IFNULL((SELECT SUM(Y.importe) FROM facturista_estado_de_cuenta Y WHERE Y.facturista_id=P.ID AND Y.FECHA<$P{FECHA_CORTE}),0) AS SALDO
,0.0 AS INT_E_IVA
,IFNULL((SELECT SUM(Y.importe) FROM facturista_estado_de_cuenta Y WHERE Y.facturista_id=P.ID AND Y.CONCEPTO='MATERIAL'  AND Y.FECHA = $P{FECHA_CORTE} ),0) AS MATERIAL
,IFNULL((SELECT SUM(Y.importe) FROM facturista_estado_de_cuenta Y WHERE Y.facturista_id=P.ID AND Y.CONCEPTO='CELULAR'  AND Y.FECHA = $P{FECHA_CORTE} ),0) AS CELULAR
,IFNULL((SELECT SUM(Y.importe) FROM facturista_estado_de_cuenta Y WHERE Y.facturista_id=P.ID AND Y.CONCEPTO='PATIN'  AND Y.FECHA = $P{FECHA_CORTE} ),0) AS PATIN
,IFNULL((SELECT SUM(Y.importe) FROM facturista_estado_de_cuenta Y WHERE Y.facturista_id=P.ID AND Y.CONCEPTO='MANIOBRA_LOCAL'  AND Y.FECHA = $P{FECHA_CORTE} ),0) AS MANIOBRA_LOCAL
,IFNULL((SELECT SUM(Y.importe) FROM facturista_estado_de_cuenta Y WHERE Y.facturista_id=P.ID AND Y.CONCEPTO='MANIOBRA_FORANEA'  AND Y.FECHA = $P{FECHA_CORTE} ),0) AS MANIOBRA_FORANEA
,IFNULL((SELECT SUM(Y.importe) FROM facturista_estado_de_cuenta Y WHERE Y.facturista_id=P.ID AND Y.CONCEPTO='VALES'  AND Y.FECHA = $P{FECHA_CORTE} ),0) AS VALES
,IFNULL((SELECT SUM(Y.importe) FROM facturista_estado_de_cuenta Y WHERE Y.facturista_id=P.ID AND Y.CONCEPTO='OTROS' AND Y.TIPO<>'ABONO'  AND Y.FECHA = $P{FECHA_CORTE} ),0) AS OTROS
from embarque E 
JOIN envio F ON(E.ID=F.EMBARQUE_ID)
JOIN envio_comision X ON(X.ENVIO_ID=F.ID)
JOIN CHOFER C ON(C.ID=E.chofer_ID)
join facturista_de_embarque P ON(C.facturista_id=P.id)
JOIN sucursal S ON(S.ID=E.sucursal_id)
WHERE  P.ID=$P{FACTURISTA} AND DATE(X.REGRESO) BETWEEN $P{FECHA_INI} AND $P{FECHA_FIN}
UNION
SELECT P.ID,P.NOMBRE,S.NOMBRE AS SUCURSAL,C.NOMBRE AS CHOFER,X.IMPORTE_COMISION VALOR ,0 AS CANTIDAD,x.KILOS,'TRASLADO' AS VTA,0 AS FLETE,X.ID,0 MANIOBRA
,X.COMISION/100 COMISION,X.PRECIO_TONELADA,X.IMPORTE_COMISION,X.VALOR_CAJAS
,IFNULL((SELECT SUM(Y.importe) FROM facturista_estado_de_cuenta Y WHERE Y.facturista_id=P.ID AND Y.FECHA<$P{FECHA_CORTE}),0) AS SALDO
,0.0 AS INT_E_IVA
,IFNULL((SELECT SUM(Y.importe) FROM facturista_estado_de_cuenta Y WHERE Y.facturista_id=P.ID AND Y.CONCEPTO='MATERIAL'  AND Y.FECHA = $P{FECHA_CORTE} ),0) AS MATERIAL
,IFNULL((SELECT SUM(Y.importe) FROM facturista_estado_de_cuenta Y WHERE Y.facturista_id=P.ID AND Y.CONCEPTO='CELULAR'  AND Y.FECHA = $P{FECHA_CORTE} ),0) AS CELULAR
,IFNULL((SELECT SUM(Y.importe) FROM facturista_estado_de_cuenta Y WHERE Y.facturista_id=P.ID AND Y.CONCEPTO='PATIN'  AND Y.FECHA = $P{FECHA_CORTE} ),0) AS PATIN
,IFNULL((SELECT SUM(Y.importe) FROM facturista_estado_de_cuenta Y WHERE Y.facturista_id=P.ID AND Y.CONCEPTO='MANIOBRA_LOCAL'  AND Y.FECHA = $P{FECHA_CORTE} ),0) AS MANIOBRA_LOCAL
,IFNULL((SELECT SUM(Y.importe) FROM facturista_estado_de_cuenta Y WHERE Y.facturista_id=P.ID AND Y.CONCEPTO='MANIOBRA_FORANEA'  AND Y.FECHA = $P{FECHA_CORTE} ),0) AS MANIOBRA_FORANEA
,IFNULL((SELECT SUM(Y.importe) FROM facturista_estado_de_cuenta Y WHERE Y.facturista_id=P.ID AND Y.CONCEPTO='VALES'  AND Y.FECHA = $P{FECHA_CORTE} ),0) AS VALES
,IFNULL((SELECT SUM(Y.importe) FROM facturista_estado_de_cuenta Y WHERE Y.facturista_id=P.ID AND Y.CONCEPTO='OTROS' AND Y.TIPO<>'ABONO' AND Y.FECHA = $P{FECHA_CORTE} ),0) AS OTROS
FROM traslado E 
JOIN envio_comision X ON(X.traslado_id=E.ID)
JOIN CHOFER C ON(C.ID=E.chofer_ID)
join facturista_de_embarque P ON(C.facturista_id=P.id)
JOIN sucursal S ON(S.ID=E.sucursal_id)
WHERE  P.ID=$P{FACTURISTA} AND DATE(X.REGRESO) BETWEEN $P{FECHA_INI} AND $P{FECHA_FIN} 
) AS A 
GROUP BY FACTURISTA]]></queryString>

