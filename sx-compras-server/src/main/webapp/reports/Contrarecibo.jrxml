	<queryString><![CDATA[SELECT A.ID,A.FECHA AS FCRECIBO,A.COMENTARIO AS OBSERVACIONES
,(SELECT X.CLAVE FROM proveedor X WHERE B.PROVEEDOR_ID=X.ID ) AS CLAVE
,(SELECT X.NOMBRE FROM proveedor X WHERE B.PROVEEDOR_ID=X.ID ) AS PROVEEDOR
,B.FOLIO AS FACTURA,B.FECHA,B.MONEDA,B.TOTAL AS IMPORTE,B.VENCIMIENTO
FROM  contrarecibo A JOIN cuenta_por_pagar B ON (A.ID=B.contrarecibo)
WHERE A.ID=$P{ID} AND 
B.TIPO='COMPRAS'
ORDER BY B.FOLIO,B.FECHA]]></queryString>

