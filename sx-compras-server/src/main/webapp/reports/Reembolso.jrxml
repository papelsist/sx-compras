	<queryString><![CDATA[SELECT r.id,r.ID folio,r.nombre afavor,s.nombre as sucursal,r.fecha,r.fecha_de_pago ,r.moneda,r.tipo_de_cambio,r.forma_de_pago,r.comentario
,(case when d.cxp_id is null and d.comentario is not null then d.comentario else d.nombre end) as proveedor
,d.id detId,concat(ifnull(d.documento_serie,''),ifnull(d.documento_folio,'')) as documento,d.documento_fecha,r.concepto
,case when c.id is null then d.total else c.total end as total
,d.apagar,SUBSTR(c.uuid,LENGTH(c.uuid)-12,LENGTH(c.uuid)) uuid ,f.acuse
FROM rembolso R join sucursal s on(r.sucursal_id=s.id)
LEFT JOIN rembolso_det  D ON(D.rembolso_id=R.id) 
left join cuenta_por_pagar c on(d.cxp_id=c.id)
left join comprobante_fiscal f on(c.comprobante_fiscal_id=f.id)
WHERE R.ID=$P{ID}]]></queryString>

